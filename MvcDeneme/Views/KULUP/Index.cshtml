@using MvcDeneme.Models.EntitiyFramework;
@model List<TBLKULUPLER>
@{
    ViewBag.Title = "Kulupler";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>Kulup Id</th>
        <th>Kulup Adı</th>
        <th>İŞLEMLER</th>
    </tr>
    <tbody>
        @foreach (var klp in Model)
        {
           <tr>
               <td>@klp.KULUPID</td>
               <td>@klp.KULUPAD</td>
               <td><a href="/KULUP/Sil/@klp.KULUPID" class="btn btn-danger">Sil</a>
               <a href="/KULUP/KulupGetir/@klp.KULUPID" class="btn btn-success">Güncelle</a></td>
           </tr>
        }
    </tbody>
</table>
<a href="/KULUP/KulupEkleme" class="btn btn-info">
    Yeni Kulüp Ekle
</a>


